public class MyClass {
    private String mName ;
    private int [] mData ;

    public MyClass ( MyClass toCopy ) {
        this.mName = toCopy.mName;
        if (toCopy.mData != null) {
            this.mData = new int[toCopy.mData.length];
            for (int i = 0; i < toCopy.mData.length; i++) {
                this.mData[i] = toCopy.mData[i];         // copy each element
            }
        } else {
            this.mData = null;
        }
    }
    @Override
    public Object clone() {
        try{
            MyClass copy = (MyClass) super.clone();
            copy.mData = mData != null ? mData.clone() : null;
            // weird syntax that chatgpt suggested. Could we go over it please?
            return copy;

        }catch (CloneNotSupportedException e){
            return null;
        }

    }
}
